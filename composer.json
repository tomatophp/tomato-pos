{
    "name": "tomatophp/tomato-pos",
    "type": "library",
    "description": "Full POS system for tomato ordering and inventory",
    "keywords": [
        "php",
        "laravel",
        "pos",
        "erp",
        "crm",
        "tomatophp",
        "tomato-ecommerce"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoPos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoPos\\TomatoPosServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "milon/barcode": "^10.0",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-ecommerce": "^1.0",
        "tomatophp/tomato-branches": "^1.0",
        "tomatophp/tomato-orders": "^1.0",
        "tomatophp/tomato-crm": "^1.1",
        "tomatophp/tomato-products": "^1.0",
        "tomatophp/tomato-coupons": "^1.0",
        "tomatophp/tomato-wallet": "^1.0",
        "tomatophp/tomato-category": "^1.1"
    }
}
